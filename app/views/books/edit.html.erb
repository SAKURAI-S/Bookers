<%= flash[:success] %>
<h1>Editing Book</h1>
<%= form_for(@book, url:"/books/#{@book.id}") do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title %>
    <h4>Body</h4>
    <%= f.text_area :body %>
   <h5><%= f.submit 'Update Book' %></h5>
<% end %>
<%= link_to 'Show', book_path(@book) %>
    <%= link_to 'Back', books_path %>