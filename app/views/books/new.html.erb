<h1>New book</h1>
<!-- form_for部分 -->
<%= form_for (@book) do |f| %>

    <h4>Title</h4>
    <%= f.text_field :title %>
	<%= render 'layouts/error_messages', model: f.object %>
    <h4>Body</h4>
    <%= f.text_area :body %>

    <%= f.submit 'Create Book' %>
	<%= render 'layouts/error_messages', model: f.object %>
<% end %>